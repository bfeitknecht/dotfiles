tmux display-menu -T "#[align=centre] Handling Pane "         -x "C" -y "C" "Back to Main menu <--" Left "run-shell '/Users/bf/.config/tmux/plugins/tmux-menus/items/main.sh'" "Move pane         -->" M "run-shell '/Users/bf/.config/tmux/plugins/tmux-menus/items/pane_move.sh'" "Resize pane       -->" R "run-shell '/Users/bf/.config/tmux/plugins/tmux-menus/items/pane_resize.sh'" "" " Set Title" t "command-prompt -I '#T'  -p 'Title: '              'select-pane -T \"%%\"' ; run-shell '/Users/bf/.config/tmux/plugins/tmux-menus/items/panes.sh'" " Clear history & screen" c "send-keys C-l ; run 'sleep 0.3' ; clear-history" "<P> Display pane numbers" q "display-panes ; run-shell '/Users/bf/.config/tmux/plugins/tmux-menus/items/panes.sh'" "<P> Copy mode - "history"" [ "copy-mode" " Display pane size" s "display-message             'Pane: #P size: #{pane_width}x#{pane_height}' ; run-shell '/Users/bf/.config/tmux/plugins/tmux-menus/items/panes.sh'"
